        <form
          id="inquiry-form"
          action="https://formspree.io/f/xgvnkdap"
          method="POST"
        >
          <div class="form-group">
            <label for="name">Your Name</label>
            <input
              type="text"
              id="name"
              name="name"
              placeholder="John Doe"
              required
            />
          </div>
          <div class="form-group">
            <label for="email">Your Email</label>
            <input
              type="email"
              id="email"
              name="email"
              placeholder="john.doe@example.com"
              required
            />
          </div>
          <div class="form-group">
            <label for="message">Your Message</label>
            <textarea
              id="message"
              name="message"
              placeholder="I'm interested in a custom guitar strap..."
              required
            ></textarea>
          </div>
          <div class="form-actions">
            <button type="submit" class="cta">Send Inquiry</button>
            <button
              type="button"
              id="whatsapp-send"
              class="cta whatsapp-inline"
              aria-label="Send enquiry via WhatsApp"
            >
